<%= form_for project do |f| %>
<%= render 'shared/errors', obj: project %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
    <%= f.submit project.new_record? ? "Create Project" : "Update Project" %>

<% end %>
